 **************** Erase **************** 

test[it]= 0 : 600
test[it]= 1 : 100
test[it]= 2 : 200
test[it]= 3 : 300
test[it]= 4 : 400
test[it]= 5 : 500

 **************** Iterators **************** 

it4= 0 : 600
it5= 0 : 600
it4--= 0 : 100
it5--= 0 : 100
it4++= 0 : 100
it5++= 0 : 100
it4++= 2 : 300
it5++= 2 : 300
it4--= 4 : 300
it5--= 4 : 300
